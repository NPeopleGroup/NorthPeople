<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
    "classpath://org/hibernate/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hyd.northpj.entity.Answer" table="t_answer">
		<id name="sn" column="sn" type="int">
			<generator class="increment" />
		</id>
		<property name="username" column="username" />
		<property name="questionId" column="questionId" />
		<property name="answer" column="answer" />
		<property name="questionSn" column="questionSn" />
		<property name="questionQuestion" column="questionQuestion" />
		<property name="questionType" column="questionType" />
		<property name="questionChoice" column="questionChoice" />
		<property name="questionFrom" column="questionFrom" />
		<property name="questionGoto" column="questionGoto" />
		<property name="questionScore" column="questionScore" />
		<property name="questionObjective" column="questionObjective" />
		<property name="questionFile" column="questionFile" />
		<property name="questionDepartment" column="questionDepartment" />
		<property name="questionHint" column="questionHint" />
	</class>
</hibernate-mapping>
  